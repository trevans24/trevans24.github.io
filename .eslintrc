{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["unused-imports"],
	"extends": [
		"eslint:recommended",
		"plugin:prettier/recommended",
		"react-app",
		"react-app/jest"
	],
	"rules": {
		"indent": ["error", 2],
		"quotes": ["warn", "single"],
		"react/jsx-uses-react": "off",
		"react/react-in-jsx-scope": "off"
	}
}
